            Label:
                text: root.current_datetime
                font_size: 20
                size_hint: None, None
                pos_hint: {"x": 0.85, "y": 0.88}

            # todo: largest stock, best selling, lowest stock,

            Label:
                text: "Largest Stock"
                font_size: 30
                size_hint: None, None
                pos_hint: {"x": 0.1, "y": 0.5}
                bold: True

            MDIcon:
                icon: "food-apple"
                size_hint: None, None
                size: "150dp", "150dp"
                pos_hint: {"x": 0.13, "y": 0.47}
                font_size: 50

            Label:
                text: "Vegetables"
                font_size: 50
                size_hint: None, None
                pos_hint: {"x": 0.1, "y": 0.6}
                bold: True

            Label:
                text: "Best Selling"
                font_size: 30
                size_hint: None, None
                pos_hint: {"x": 0.45, "y": 0.5}
                bold: True

            MDIcon:
                icon: "cash"
                size_hint: None, None
                size: "150dp", "150dp"
                pos_hint: {"x": 0.48, "y": 0.47}
                font_size: 50

            Label:
                text: "Junk Foods"
                font_size: 50
                size_hint: None, None
                pos_hint: {"x": 0.45, "y": 0.6}
                bold: True

            Label:
                text: "Lowest Stock"
