<SplashWindow>:
    name: "splash"

    BoxLayout:
        orientation: "vertical"
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size

        Image:
            padding: 0
            source: "images/splashfinal.png"
            size_hint: None, None
            size: root.width, root.height
            allow_stretch: True
            keep_ratio: False

        BoxLayout:
            orientation: "vertical"
            size_hint_y: None
            height: dp(30)

            ProgressBar:
                id: progress_bar
                max: 100
                value: 0
                size_hint_x: None
                width: self.parent.width * 1
                height: dp(10)

