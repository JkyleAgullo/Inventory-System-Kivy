#:import NoTransition kivy.uix.screenmanager.NoTransition

<WindowManager>:
    SplashWindow:
    AdminDB:
    AdminADD:
    AdminDisplay:
    AdminSettings:
    AdminLoginTry:

<AdminDB>:
    name: "adminDB"
    on_leave:
        root.ids.txt.text = ''
        root.ids.qty.text = ''
        #you also need to clear the object where the input is stored, refer to your bookmarked link
    name:"first"
    GridLayout:
        cols:2
        size: root.width, root.height

        GridLayout:
            spacing: 10
            cols: 1
            size_hint: 0.2,1

            Button: # Empty Button
                text:""
                background_normal: ''
                background_down: ''
                background_color:(0/255,0/255,0/255,.7)
                size_hint: None, None
                size: 150, 130

            Button:
                id: DB
                text:"DASHBOARD"
                background_normal: ''
                background_down: ''
                color: 1,1,1,1
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50

            Button:
                id: ADD
                text:"ADD"
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "second"


            Button:
                id: DP
                text:"DISPLAY"
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "third"

            Button:
                id: ST
                text:"SETTING"
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "fourth"

            Button:
                text:""
                background_normal: ''
                background_down: ''
                background_color:(0/255,0/255,0/255,.7)
             #              color: 0,0,0,1
                size_hint: None, None
                size: 150, 340

            Button:
                id: LOGOUT
                text:"LOGOUT"
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50

        FloatLayout:
            id:dashboard
            size: root.width, root.height

            TextInput:
                id: txt
                #text: "hi"
                size_hint: 0.2,.05
                pos_hint: {"x":.2,"y":.5}
                multiline: False
                on_text_validate: root.text_validate(self)
            TextInput:
                id: qty
                #text: "hi"
                size_hint: 0.2,.05
                pos_hint: {"x":.5,"y":.6}
                multiline: False
                on_text_validate: root.qty_validate(self)
            Label:
                text: root.text_input_string
                color: 1,1,1,1
            Label:
                text: root.qty_input_string
                color: 1,1,1,1
                size_hint: 0.2,.05
                pos_hint: {"x":.3,"y":.6}
            Label:
                text: 'Dashboard'
                color: 1,1,1,1
                size_hint: 0.2,.05
                pos: 300,200

            Button:
                text: 'enter'
                size_hint: 0.2,.05
                pos_hint: {"x":.3,"y":.7}
                on_press: root.qty_validate(qty)
                on_press: root.text_validate(txt)



