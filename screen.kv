<AdminDB>:
    id: adminDB
    name:"first"
    GridLayout:
        cols:2
        size: root.width, root.height

        GridLayout:
            cols: 1
            size_hint: 0.2,1

            Button: # Empty Button
                text:""
                background_normal: ''
                background_down: ''
                background_color:(0/255,0/255,0/255,.7)
                size_hint: None, None
                size: 150, 130

            Button:
                id: DB
                text:"DASHBOARD"
                bold: True
                background_normal: ''
                background_down: ''
                color: 1,1,1,1
                background_color:(143/255,195/255,31/255,1)
                size_hint: None, None
                size: 150, 50

            Button:
                id: ADD
                text:"ADD"
                bold: True
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                #background_color:(143/255,195/255,31/255,1)
                background_color: 0,0,0,1
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "second"


            Button:
                id: DP
                text:"DISPLAY"
                bold: True
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                #background_color:(143/255,195/255,31/255,1)
                background_color: 0,0,0,1
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "third"

            Button:
                id: ST
                text:"SETTING"
                bold: True
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                #background_color:(143/255,195/255,31/255,1)
                background_color: 0,0,0,1
                size_hint: None, None
                size: 150, 50
                on_press:
                    root.manager.transition = NoTransition()
                    root.manager.current = "fourth"

            Button:
                text:""
                background_normal: ''
                background_down: ''
                background_color:(0/255,0/255,0/255,.7)
                size_hint: None, None
                size: 150, 520

            Button:
                id: LOGOUT
                text:"LOGOUT"
                bold: True
                color: 1,1,1,1
                background_normal: ''
                background_down: ''
                #background_color:(143/255,195/255,31/255,1)
                background_color: 0,0,0,1
                size_hint: None, None
                size: 150, 50

        FloatLayout:
            id: adminDashboard
            size: root.width, root.height

            Label:
                text:"Welcome, Admin"
                font_size: 35
                size_hint: None, None
                pos_hint: {"x": 0.1, "y": 0.88}
